{
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": true,
    "strictNullChecks": false,
    "declaration": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2016", "dom"],
    "jsx": "react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
  },
  "include": ["./src/**/*"]
}
